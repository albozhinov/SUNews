@model UserIndexViewModel

<partial name="_UserGrid" model="@Model.Users" />

@section Scripts{
<script>
    $(document).on("click", "#contentPager a", function () {
        $.ajax({
            url: $(this).attr("href"),
            type: 'GET',
            cache: false,
            success: function (result) {
                $('#content').html(result);
            }
        });
        return false;
    });
</script>
}
